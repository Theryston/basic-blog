<%- include ('partials/header') %>
<%- include ('partials/homenavbar') %>
<body>
	<div class="container mt-4">
		
		<div class="jumbotron">
	  	<h1 class="display-4">Bem-vindo!</h1>
		 	<p class="lead">Este é um simples blog feito por Theryston!</p>
		 	<hr class="my-4">
		 	<p>Descrição destelhada</p>
			<a class="btn btn-primary btn-lg" href="#" role="button">Leia mais!</a>
	</div>
		
		<hr>
		<% articles.forEach((article) => { %>
		<div class="card">
			<div class="card-header">
				<%= article.title %>
			</div>
			<div class="card-body">
				<a href="#" class="btn btn-primary">Ler artigo</a>
			</div>
		</div><br>
		<% }) %>
	</div>
</body>
<%- include ('partials/footer') %>